{
  "extends": ["tslint-react", "tslint-config-airbnb"],
  "rules": {
    "variable-name": [true, "allow-pascal-case"],
    "trailing-comma": [
      true,
      {
        "multiline": false,
        "singleline": "never"
      }
    ],
    "max-line-length": [
      true,
      150
    ],
    "import-name": false
  }
}
